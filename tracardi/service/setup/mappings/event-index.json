{
  "index_patterns": [
    "%%TEMPLATE_PATTERN%%"
  ],
  "template": {
    "settings": {
    "number_of_shards": %%SHARDS%%,
    "number_of_replicas": %%REPLICAS%%,
      "index.mapping.total_fields.limit": 2000,
      "index.refresh_interval": "10s"
    },
    "mappings": {
      "_meta": {
        "version": "%%VERSION%%",
        "name": "%%PREFIX%%"
      },
      "dynamic": "strict",
      "properties": {
        "id": {
          "type": "keyword"
        },
        "name": {
          "type": "keyword"
        },
        "metadata": {
          "properties": {
            "aux": {
              "type": "flattened"
            },
            "time": {
              "properties": {
                "insert": {
                  "type": "date"
                },
                "create": {
                  "type": "date"
                },
                "process_time": {
                  "type": "float"
                }
              }
            },
            "status": {
              "type": "keyword",
              "null_value": "NULL"
            },
            "channel": {
              "type": "keyword"
            },
            "ip": {
              "type": "keyword"
            },
            "processed_by": {
              "properties": {
                "rules": {
                  "type": "keyword"
                },
                "flows": {
                  "type": "keyword"
                },
                "third_party": {
                  "type": "keyword"
                }
              }
            },
            "profile_less": {
              "type": "boolean"
            },
            "valid": {
              "type": "boolean"
            },
            "warning": {
              "type": "boolean"
            },
            "error": {
              "type": "boolean"
            },
            "instance": {
              "properties": {
                "id": {
                   "type": "keyword"
                }
              }
            },
            "debug": {
              "type": "boolean"
            }
          }
        },
        "type": {
          "type": "keyword",
          "null_value": "NULL"
        },
        "properties": {
          "type": "flattened"
        },
        "request": {
          "enabled": false,
          "dynamic": "true",
          "type": "object"
        },
        "source": {
          "properties": {
            "id": {
              "type": "keyword"
            }
          }
        },

        "device": {
          "properties": {
            "name": {
              "type": "keyword"
            },
            "brand": {
              "type": "keyword"
            },
            "model": {
              "type": "keyword"
            },
            "ip": {
              "type": "keyword",
              "ignore_above": 18
            },
            "type": {
              "type": "keyword"
            },
            "touch": {
              "type": "boolean"
            },
            "resolution": {
              "type": "keyword",
              "ignore_above": 32
            },
            "color_depth": {
              "type": "integer"
            },
            "orientation": {
              "type": "keyword",
              "ignore_above": 32
            },
            "geo": {
              "properties": {
                "country": {
                  "properties": {
                    "name": {
                      "type": "keyword"
                    },
                    "code": {
                      "type": "keyword",
                      "ignore_above": 10
                    }
                  }
                },
                "county": {
                   "type": "keyword",
                   "ignore_above": 64
                },
                "city": {
                  "type": "keyword",
                   "ignore_above": 64
                },
                "latitude": {
                  "type": "float"
                },
                "longitude": {
                  "type": "float"
                },
                "postal": {
                  "type": "keyword",
                  "ignore_above": 24
                }
              }
            }
          }
        },
        "os": {
          "properties": {
            "name": {
              "type": "keyword",
              "ignore_above": 64
            },
            "version": {
              "type": "keyword",
              "ignore_above": 32
            }
          }
        },
        "app": {
          "properties": {
            "type": {
              "type": "keyword"
            },
            "bot": {
              "type": "boolean"
            },
            "name": {
              "type": "keyword"
            },
            "version": {
              "type": "keyword"
            },
            "language": {
              "type": "keyword",
              "ignore_above": 32
            },
            "resolution": {
              "type": "keyword",
              "ignore_above": 32
            }
          }
        },
        "hit": {
          "properties": {
            "name": {
              "type": "keyword"
            },
            "url": {
              "type": "keyword"
            },
            "referer": {
              "type": "keyword"
            },
            "query": {
              "type": "keyword"
            }
          }
        },
        "utm": {
          "properties": {
            "source": {
              "type": "keyword",
              "ignore_above": 64
            },
            "medium": {
              "type": "keyword",
              "ignore_above": 64
            },
            "campaign": {
              "type": "keyword",
              "ignore_above": 64
            },
            "term": {
              "type": "keyword",
              "ignore_above": 64
            },
            "content": {
              "type": "keyword",
              "ignore_above": 96
            }
          }
        },
        "history": {
          "properties": {
            "page": {
              "properties": {
                "name": {
                  "type": "keyword"
                },
                "url": {
                  "type": "keyword"
                },
                "referer": {
                  "type": "keyword"
                },
                "query": {
                  "type": "keyword"
                },
                "category": {
                  "type": "keyword",
                  "ignore_above": 64
                }
              }
            },
            "order": {
              "properties": {
                "id": {
                  "type": "keyword"
                },
                "name": {
                  "type": "keyword"
                }
              }
            }
          }
        },

        "session": {
          "properties": {
            "id": {
              "type": "keyword"
            },
            "start": {
              "type": "date"
            },
            "duration": {
              "type": "float"
            },
            "tz": {
              "type": "keyword"
            }
          }
        },
        "profile": {
          "dynamic": "false",
          "properties": {
            "id": {
              "type": "keyword"
            }
          }
        },
        "aux": {
          "type": "flattened"
        },
        "config": {
          "dynamic": "true",
          "type": "object",
          "enabled": false
        },
        "context": {
          "type": "flattened"
        },
        "traits": {
          "dynamic": "true",
          "type": "object"
        },
        "tags": {
          "properties": {
            "values": {
              "type": "keyword"
            },
            "count": {
              "type": "double"
            }
          }
        }
      }
    },
    "aliases": {
      "%%ALIAS%%": {}
    }
  }
}