{
  "index_patterns": [
    "%%VERSION%%.%%PREFIX%%.tracardi-event-*-*"
  ],
  "template": {
    "settings": {
    "number_of_shards": %%SHARDS%%,
    "number_of_replicas": %%REPLICAS%%,
      "index.mapping.total_fields.limit": 2000,
      "index.refresh_interval": "10s"
    },
    "mappings": {
      "_meta": {
        "version": "%%VERSION%%",
        "name": "%%PREFIX%%"
      },
      "dynamic": "strict",
      "properties": {
        "id": {
          "type": "keyword"
        },
        "metadata": {
          "properties": {
            "aux": {
              "type": "flattened"
            },
            "time": {
              "properties": {
                "insert": {
                  "type": "date"
                },
                "create": {
                  "type": "date"
                },
                "process_time": {
                  "type": "float"
                }
              }
            },
            "status": {
              "type": "keyword",
              "null_value": "NULL"
            },
            "channel": {
              "type": "keyword"
            },
            "ip": {
              "type": "keyword"
            },
            "processed_by": {
              "properties": {
                "rules": {
                  "type": "keyword"
                },
                "third_party": {
                  "type": "keyword"
                }
              }
            },
            "profile_less": {
              "type": "boolean"
            },
            "valid": {
              "type": "boolean"
            },
            "warning": {
              "type": "boolean"
            },
            "error": {
              "type": "boolean"
            },
            "instance": {
              "properties": {
                "id": {
                   "type": "keyword"
                }
              }
            },
            "debug": {
              "type": "boolean"
            }
          }
        },
        "type": {
          "type": "keyword",
          "null_value": "NULL"
        },
        "properties": {
          "type": "flattened"
        },
        "request": {
          "enabled": false,
          "dynamic": "true",
          "type": "object"
        },
        "source": {
          "properties": {
            "id": {
              "type": "keyword"
            }
          }
        },
        "session": {
          "properties": {
            "id": {
              "type": "keyword"
            },
            "start": {
              "type": "date"
            },
            "duration": {
              "type": "float"
            },
            "tz": {
              "type": "keyword"
            }
          }
        },
        "profile": {
          "dynamic": "false",
          "properties": {
            "id": {
              "type": "keyword"
            }
          }
        },
        "aux": {
          "type": "flattened"
        },
        "config": {
          "dynamic": "true",
          "type": "object",
          "enabled": false
        },
        "context": {
          "type": "flattened"
        },
        "traits": {
          "dynamic": "true",
          "type": "object"
        },
        "tags": {
          "properties": {
            "values": {
              "type": "keyword"
            },
            "count": {
              "type": "double"
            }
          }
        }
      }
    },
    "aliases": {
      "%%ALIAS%%": {}
    }
  }
}