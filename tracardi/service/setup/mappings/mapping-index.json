{
  "settings": {
    "number_of_shards": 5,
    "number_of_replicas": 1
  },
  "mappings": {
    "dynamic": false,
    "properties": {
      "date": {
        "type": "date"
      },
      "version": {
        "type": "keyword"
      },
      "index_name": {
        "type": "keyword"
      },
      "index": {
        "type": "keyword"
      },
      "mapping": {
        "enabled": false,
        "type": "object"
      }
    }
  },
  "aliases": {
    "%%ALIAS%%": {
      "index": "%%VERSION%%.%%ALIAS%%"
    }
  }
}
