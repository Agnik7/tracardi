{
  "settings": {
    "number_of_shards": %%CONF_SHARDS%%,
    "number_of_replicas": %%REPLICAS%%
  },
  "mappings": {
    "_meta": {
      "version": "%%VERSION%%",
      "name": "%%PREFIX%%"
    },
    "dynamic": "strict",
    "properties": {
      "id": {
        "type": "keyword"
      },
      "version": {
        "type": "keyword"
      },
      "name": {
        "type": "keyword"
      },
      "prev_version": {
        "type": "object",
        "enabled": false
      },
      "upgrades": {
        "type": "keyword"
      }
    }
  }
}

