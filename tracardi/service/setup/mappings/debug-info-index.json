{
  "index_patterns": [
    "%%VERSION%%.%%PREFIX%%.tracardi-debug-info"
  ],
  "data_stream": {},
  "template": {
    "settings": {
      "number_of_shards": %%SHARDS%%,
      "number_of_replicas": %%REPLICAS%%,
      "index.lifecycle.rollover_alias": "%%VERSION%%.%%PREFIX%%.tracardi-debug-info-stream"
    },
    "mappings": {
      "_meta": {
        "version": "%%VERSION%%",
        "name": "%%PREFIX%%"
      },
      "dynamic": "false",
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "date_optional_time||epoch_millis"
        },
        "id": {
          "type": "keyword"
        },
        "content": {
          "type": "keyword",
          "index": false
        }
      }
    }
  }
}