{
  "index_patterns": [
    "%%VERSION%%.%%PREFIX%%.tracardi-user-log-*-*"
  ],
  "template": {
    "settings": {
    "number_of_shards": %%SHARDS%%,
    "number_of_replicas": %%REPLICAS%%
    },
    "mappings": {
      "dynamic": false,
      "properties": {
        "timestamp": {
          "type": "date"
        },
        "successful": {
          "type": "boolean"
        },
        "email": {
          "type": "keyword"
        }
      }
    }
  }
}