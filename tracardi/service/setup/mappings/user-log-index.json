{
  "index_patterns": [
    "%%VERSION%%.%%PREFIX%%.tracardi-user-log"
  ],
  "data_stream": {},
  "template": {
    "settings": {
      "number_of_shards": %%SHARDS%%,
      "number_of_replicas": %%REPLICAS%%,
      "index.lifecycle.rollover_alias": "%%VERSION%%.%%PREFIX%%.tracardi-user-log-stream"
    },
    "mappings": {
      "_meta": {
        "version": "%%VERSION%%",
        "name": "%%PREFIX%%"
      },
      "dynamic": "false",
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "date_optional_time||epoch_millis"
        },
        "timestamp": {
          "type": "date"
        },
        "successful": {
          "type": "boolean"
        },
        "email": {
          "type": "keyword"
        }
      }
    }
  }
}